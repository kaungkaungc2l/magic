<h1>New Project</h1>

<%= form_with model: @project do |form| %>
  <div>
    <%= form.label :project_name %><br>
    <%= form.text_field :project_name %>
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>